<h1><%= @doc.title%></h1>
<table class="mdl-data-table mdl-js-data-table">
  <tr>
    <th>
      Doc Id
    </th>
    <th>
      Status
    </th>
    <th>
      Date
    </th>
    <th>
      Title
    </th>
  </tr>
  <tr>
    <th>
    <%=@doc.id%>
    </th>
    <th>
      <%=@doc.status%>
    </th>
    <th>
      <%=@doc.date%>
    </th>
    <th>
      <%=@doc.title%>
    </th>
  </tr>
</table>


<iframe src="<%= attachment_url(@doc, :doc, format: 'pdf') %>" width="400px" height="600px"></iframe>

<h5>Client Commment</h5>
<%= render partial: 'client_comment'%>

<div class="mdl-layout__content">
  <div class="mdl-page-content">
    <h5>Upload Document</h5>
    <%= render partial: 'upload'%>
  </div>
</div>
