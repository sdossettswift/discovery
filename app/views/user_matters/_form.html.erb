<h1>Assign Users-Roles to Matters</h1>
<h2>Admin Access Only</h2>

<h3>Select Case</h3>

<%=simple_form_for @user_matter do |f|%>
  <%= f.association :matter, label_method: :cause_number, as: :check_boxes%>
  <%= f.association :user, label_method: :username, as: :check_boxes %>
  <%= f.button :submit%>
<%end%>
